{
  "name": "hiberfile",
  "version": "2021.3.0",
  "private": true,
  "scripts": {
    "current-git-sha": "NUXT_ENV_CURRENT_GIT_SHA=`git rev-parse --short HEAD`",
    "dev": "npm run current-git-sha nuxt-ts",
    "test": "jest",
    "build": "nuxt-ts build",
    "start": "npm run current-git-sha nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/color-mode": "^2.0.5",
    "@nuxtjs/dotenv": "^1.4.1",
    "@types/crypto-js": "^4.0.1",
    "@types/qrcode": "^1.4.0",
    "axios": "^0.21.2",
    "core-js": "^3.11.0",
    "crypto-js": "^4.0.0",
    "dropzone": "^5.9.2",
    "eslint-plugin-vue": "^7.18.0",
    "jszip": "^3.7.0",
    "nuxt": "^2.15.4",
    "nuxt-i18n": "^6.26.0",
    "nuxt-property-decorator": "^2.9.1",
    "qrcode": "^1.4.4",
    "queue": "^6.0.2",
    "vue-pets": "^1.0.18",
    "vue-sweetalert2": "^4.3.1"
  },
  "devDependencies": {
